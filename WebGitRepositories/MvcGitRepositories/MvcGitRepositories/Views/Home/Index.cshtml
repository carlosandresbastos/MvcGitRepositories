@{
    ViewBag.Title = "Bem-vindo";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
            <p>

        </div>
    </section>
}
<h3></h3>
<ol class="round">
    <li class="one">
        <h5>@Html.ActionLink("Meus repositórios", "MyRepositories", "Repositories")</h5>
        Listado de todos os repositórios do Carlos Bastos.
       
    </li>

    <li class="two">
        <h5>@Html.ActionLink("Procurar repositórios", "SearchRepositories", "Repositories")</h5>
        Procurar por repositórios do <strong>github</strong> utilizando como filtro nome ou parte do nome do repositório.
    </li>

    <li class="three">
        <h5>@Html.ActionLink("Repositórios favoritos", "About", "Home")</h5>
        Listado todos os repositórios favoritos.
        
    </li>
</ol>
